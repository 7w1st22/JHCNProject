        <result property="email"    column="email"    />
        <result property="phone"    column="phone"    />
    </resultMap>

    <sql id="selectJhcnEmployeeVo">
        select id, name, credential_type, credential_no, name_py, employee_type, company_name, job_source, work_place, social_security_payment_location, sex, birthday, ethn, marriage_status, nation, registered_residence_nature, permanent_residence, permanent_residence_address, native_place, birth_place, home_address, home_address_info, work_start, job_start, email, phone from jhcn_employee
    </sql>

    <select id="selectJhcnEmployeeList" parameterType="JhcnEmployee" resultMap="JhcnEmployeeResult">
        <include refid="selectJhcnEmployeeVo"/>
        <where>  
            <if test="name != null  and name != ''"> and name like concat('%', #{name}, '%')</if>
            <if test="credentialType != null  and credentialType != ''"> and credential_type = #{credentialType}</if>
            <if test="credentialNo != null  and credentialNo != ''"> and credential_no like concat('%', #{credentialNo}, '%')</if>
            <if test="namePy != null  and namePy != ''"> and name_py like concat('%', #{namePy}, '%')</if>
            <if test="employeeType != null  and employeeType != ''"> and employee_type = #{employeeType}</if>
            <if test="companyName != null  and companyName != ''"> and company_name like concat('%', #{companyName}, '%')</if>
            <if test="jobSource != null  and jobSource != ''"> and job_source = #{jobSource}</if>
            <if test="workPlace != null  and workPlace != ''"> and work_place like concat('%', #{workPlace}, '%')</if>
            <if test="socialSecurityPaymentLocation != null  and socialSecurityPaymentLocation != ''"> and social_security_payment_location like concat('%', #{socialSecurityPaymentLocation}, '%')</if>
            <if test="sex != null  and sex != ''"> and sex = #{sex}</if>
            <if test="birthday != null  and birthday != ''"> and birthday = #{birthday}</if>
            <if test="ethn != null  and ethn != ''"> and ethn = #{ethn}</if>
            <if test="marriageStatus != null  and marriageStatus != ''"> and marriage_status = #{marriageStatus}</if>
            <if test="nation != null  and nation != ''"> and nation = #{nation}</if>
            <if test="registeredResidenceNature != null  and registeredResidenceNature != ''"> and registered_residence_nature = #{registeredResidenceNature}</if>
            <if test="permanentResidence != null  and permanentResidence != ''"> and permanent_residence like concat('%', #{permanentResidence}, '%')</if>
            <if test="permanentResidenceAddress != null  and permanentResidenceAddress != ''"> and permanent_residence_address like concat('%', #{permanentResidenceAddress}, '%')</if>
            <if test="nativePlace != null  and nativePlace != ''"> and native_place like concat('%', #{nativePlace}, '%')</if>
            <if test="birthPlace != null  and birthPlace != ''"> and birth_place like concat('%', #{birthPlace}, '%')</if>
            <if test="homeAddress != null  and homeAddress != ''"> and home_address like concat('%', #{homeAddress}, '%')</if>
            <if test="homeAddressInfo != null  and homeAddressInfo != ''"> and home_address_info like concat('%', #{homeAddressInfo}, '%')</if>
            <if test="workStart != null  and workStart != ''"> and work_start = #{workStart}</if>
            <if test="jobStart != null  and jobStart != ''"> and job_start = #{jobStart}</if>
            <if test="email != null  and email != ''"> and email like concat('%', #{email}, '%')</if>
            <if test="phone != null  and phone != ''"> and phone like concat('%', #{phone}, '%')</if>
        </where>
    </select>
    
    <select id="selectJhcnEmployeeById" parameterType="Long" resultMap="JhcnEmployeeResult">
        <include refid="selectJhcnEmployeeVo"/>
        where id = #{id}
    </select>
    <select id="selectJhcnEmployeeByName"  parameterType="String" resultMap="JhcnEmployeeResult">
        <include refid="selectJhcnEmployeeVo"/>
        where `name` = #{name}
    </select>
    <select id="selectJhcnEmployeeByCardNo" parameterType="String" resultMap="JhcnEmployeeResult">
        <include refid="selectJhcnEmployeeVo"/>
        where credential_no = #{credentialNo}
    </select>

