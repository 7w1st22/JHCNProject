    </update>

    <delete id="deleteJhcnEmployeeById" parameterType="Long">
        delete from jhcn_employee where id = #{id}
    </delete>

    <delete id="deleteJhcnEmployeeByIds" parameterType="String">
        delete from jhcn_employee where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
