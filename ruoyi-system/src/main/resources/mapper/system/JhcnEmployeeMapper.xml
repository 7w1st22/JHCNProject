<mapper namespace="com.ruoyi.system.mapper.JhcnEmployeeMapper">
    
    <resultMap type="JhcnEmployee" id="JhcnEmployeeResult">
        <result property="id"    column="id"    />
        <result property="name"    column="name"    />
        <result property="credentialType"    column="credential_type"    />
        <result property="credentialNo"    column="credential_no"    />
